// CPP program to print all occurrences of a pattern
// in a text
#include <bits/stdc++.h>
using namespace std;

bool printOccurrences(string txt, string pat)
{
	int found = txt.find(pat);
	while (found != string::npos) {
	   return true;
	}
	return false;
}

int main()
{
	string txt = "aaaa", pat = "aa";
	cout<<printOccurrences(txt, pat);
	return 0;
}
