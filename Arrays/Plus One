class Solution {
  public:
    vector<int> increment(vector<int> arr ,int N) {
        vector<int>result;
        int ans=0;
        for(int i=0;i<N;i++)
        {
            if(arr[i]>0)
            {
                ans=ans*10+arr[i];
            }
        }
        ans=ans+1;
        result.push_back(ans);
        return result;
    }
};
